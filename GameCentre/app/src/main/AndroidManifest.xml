<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fall2018.csc2017.gamecentre">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="fall2018.csc2017.gamecentre.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="fall2018.csc2017.gamecentre.GameActivity"></activity>
        <activity android:name="fall2018.csc2017.gamecentre.StartingActivity" />
        <activity android:name="fall2018.csc2017.gamecentre.RegisterActivity" />
        <activity android:name = "fall2018.csc2017.gamecentre.PreStartingActivity" />
        <activity android:name = "fall2018.csc2017.gamecentre.SelectingGameActivity" />
        <activity android:name="fall2018.csc2017.gamecentre.PreScoreBoardActivity" />
        <activity android:name="fall2018.csc2017.gamecentre.ScoreBoardActivity" />
        <activity android:name="fall2018.csc2017.gamecentre.ChooseBackgroundActivity" />
        <activity android:name="fall2018.csc2017.gamecentre.InstructionsActivity" />
        <activity android:name="fall2018.csc2017.gamecentre.GameAppCompatActivity"/>
        <activity android:name="fall2018.csc2017.gamecentre.gridfiller.GFGameActivity" />
        <activity android:name="fall2018.csc2017.gamecentre.minesweeper.MSGameActivity" />
        <activity android:name="fall2018.csc2017.gamecentre.minesweeper.MSPreStartingActivity" />
        <activity android:name="fall2018.csc2017.gamecentre.gridfiller.GFPreStartingActivity" />
    </application>

</manifest>